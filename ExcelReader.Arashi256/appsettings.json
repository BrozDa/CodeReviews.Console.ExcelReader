{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  // Required for the EPPlus package
  "EPPlus": {
    "ExcelPackage": {
      "LicenseContext": "NonCommercial"
    }
  },
  // Change Input to "movies-input-csv-file.csv" if you want to load from a CSV file instead of Excel. Change the Service class used in Program.cs
  "Data_File": {
    "Input": "movies-input-spreadsheet.xlsx",
    "Output": "movies-output-spreadsheet.xlsx"
  },
  "Database": {
    "ConnectionString": "Data Source=excelimporter.db"
  }
}